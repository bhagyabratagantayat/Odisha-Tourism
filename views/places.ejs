<style>
/* ===== General Styles ===== */
body {
  margin: 0;
  font-family: 'Arial', sans-serif;
  background-color: #f5f5f5;
  color: #333;
}

/* ===== Navbar ===== */
.navbar {
  background-color: #043927;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px 20px;
  position: sticky;
  top: 0;
  z-index: 1000;
}

.navbar .logo {
  display: flex;
  align-items: center;
  gap: 10px;
  font-size: 20px;
  font-weight: bold;
  color: white;
}

.navbar .logo img {
  height: 40px;
  width: auto;
  border-radius: 50%;
}

.navbar .nav-links {
  list-style: none;
  display: flex;
  gap: 20px;
}

.navbar .nav-links li a {
  color: white;
  text-decoration: none;
  font-size: 16px;
  padding: 5px 10px;
  border-radius: 4px;
  transition: background 0.3s;
}

.navbar .nav-links li a:hover,
.navbar .nav-links li a.active {
  background-color: #065a44;
}

/* ===== Search Form ===== */
.search-form {
  display: flex;
  align-items: center;
  gap: 5px;
  position: relative;
}

.search-form input {
  padding: 7px 10px;
  border-radius: 6px;
  border: none;
  font-size: 14px;
}

.search-form button {
  background-color: #065a44;
  border: none;
  padding: 8px 12px;
  color: white;
  border-radius: 6px;
  cursor: pointer;
  transition: background 0.3s;
}

.search-form button:hover {
  background-color: #043927;
}

/* ===== Search Suggestions ===== */
#suggestions {
  position: absolute;
  top: 100%;
  left: 0;
  width: 100%;
  background: #065a44;
  color: white;
  border: 1px solid #043927;
  max-height: 200px;
  overflow-y: auto;
  display: none;
  z-index: 100;
  border-radius: 6px;
  box-shadow: 0 4px 10px rgba(0,0,0,0.2);
}

#suggestions li {
  padding: 10px;
  cursor: pointer;
  transition: background 0.3s, color 0.3s;
}

#suggestions li:hover {
  background-color: #043927;
  color: #ffd700;
}

/* ===== Places Section ===== */
.places {
  max-width: 1200px;
  margin: 40px auto;
  padding: 0 20px;
}

.places h2 {
  text-align: center;
  color: #043927;
  margin-bottom: 30px;
}

/* ===== Cards Container ===== */
.cards-container {
  display: grid;
  grid-template-columns: repeat(4, 1fr); /* 4 cards per row */
  gap: 25px;
  margin: 0 auto;
}

/* ===== Individual Card ===== */
.card {
  background-color: white;
  border-radius: 12px;
  overflow: hidden;
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
  transition: transform 0.4s ease, box-shadow 0.4s ease;
  cursor: pointer;
}

.card:hover {
  transform: translateY(-8px) scale(1.02);
  box-shadow: 0 12px 25px rgba(0, 0, 0, 0.2);
}

.card h3, .card p {
  transition: color 0.4s ease;
}

.card h3 {
  margin: 15px;
  color: #043927;
}

.card p {
  margin: 0 15px 15px 15px;
  font-size: 14px;
  color: #555;
}

.card:hover h3,
.card:hover p {
  color: #065a44;
}

/* ===== Slider inside Card ===== */
.slider {
  position: relative;
  overflow: hidden;
  width: 100%;
  height: 200px;
}

.slider img {
  width: 100%;
  height: 200px;
  object-fit: cover;
  display: none;
  transition: transform 0.5s ease, opacity 0.5s ease-in-out;
}

.slider img.active {
  display: block;
}

.card:hover .slider img.active {
  transform: scale(1.05);
}

/* ===== Responsive ===== */

/* Navbar & search adjustments */
@media (max-width: 992px) {
  .navbar {
    flex-direction: column;
    align-items: flex-start;
  }

  .navbar .nav-links {
    flex-direction: column;
    gap: 10px;
    width: 100%;
    margin-top: 10px;
  }

  .search-form {
    width: 100%;
    margin-top: 10px;
  }
}

/* Cards responsiveness */
@media (max-width: 1200px) {
  .cards-container {
    grid-template-columns: repeat(3, 1fr); /* 3 cards per row */
  }
}

@media (max-width: 992px) {
  .cards-container {
    grid-template-columns: repeat(2, 1fr); /* 2 cards per row */
  }
}

@media (max-width: 600px) {
  .cards-container {
    grid-template-columns: 1fr; /* 1 card per row */
  }

  .slider {
    height: 180px;
  }
}

@media (max-width: 480px) {
  .slider {
    height: 150px;
  }
}
</style>
